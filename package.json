  "license": "MIT",
  "scripts": {
    "build": "pnpm build:chrome",
    "build:chrome": "rollup -c && pnpm lint && web-ext build -s ./static --overwrite-dest --artifacts-dir=dist/chrome",
    "watch": "rollup -c -w",
    "open": "pnpm open:chrome",
    "open:chrome": "web-ext run -s ./static --target=chromium",
    "rollup": "rollup -c",
    "test": "playwright test",
    "lint": "eslint src",
    "lint:svelte": "svelte-check"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chrome": "^0.0.217",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "mocha": "^10.0.0",
    "mock-local-storage": "^1.1.23",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "rollup": "^3.17.3",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.3",
    "sinon": "^14.0.0",
    "sinon-chrome": "^3.0.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.3.0",
    "svelte-preprocess": "^5.0.1",
    "typescript": "4.9.5",
    "web-ext": "^7.6.2"
  }
}
